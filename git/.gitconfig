[user]
	email = jesselondon@gmail.com
	name = Jesse London

[alias]
	st = status
	ci = commit
	co = checkout
	br = branch
	d = diff
	out = log origin/master..
	in = log ..origin/master
	compare = !$(dirname $(readlink $HOME/.gitconfig || echo $HOME/.gitconfig))/gitcompare.bash $1
	oldest-ancestor = !bash -c 'diff --old-line-format='' --new-line-format='' <(git rev-list --first-parent "${1:-master}") <(git rev-list --first-parent "${2:-HEAD}") | head -1' -

[color]
	ui = true
